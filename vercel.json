{
    "version": 2,
    "builds": [
        { "src": "./index.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
            "src": "/socket.io/(.*)",
            "dest": "https://new-ochre.vercel.app/socket.io/$1",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}

